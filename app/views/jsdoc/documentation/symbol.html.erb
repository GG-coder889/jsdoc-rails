<header>
  <h1><%= @symbol.symbol_type.capitalize %>: <%= link_to(@symbol.name, jsdoc_symbol_path(@symbol.alias)) %></h1>
  <dl>
    <dt>Module</dt>
    <dd><%= @symbol.member_of ? link_to(@symbol.member_of, jsdoc_symbol_path(@symbol.member_of)) : 'Global' %></dd>

    <dt>Defined In</dt>
    <dd><%= link_to(@symbol.defined_in, jsdoc_source_path(@symbol.defined_in)) %></dd>

    <dt>Class</dt>
    <dd><%= link_to(@symbol.name, jsdoc_symbol_path(@symbol.alias)) %></dd>


    <% if @symbol.subclasses.size > 0 %>
      <dt>Subclasses</dt>
      <% for subclass in @symbol.subclasses %>
        <dd><%= link_to(subclass.alias, jsdoc_symbol_path(subclass.alias)) %></dd>
      <% end %>
      
    <% end %>

    <% if @symbol.extends.present? %>
      <dt>Extends</dt>
      <dd><%= link_to(@symbol.extends, jsdoc_symbol_path(@symbol.extends)) %></dd>
    <% end %>

    <% if @symbol.requires.size > 0 %>
      <dt>Requires</dt>
      <dd><%= @symbol.requires.join(', ') %></dd>
    <% end %>
  </dl>
</header>

<section id="inheritance_tree">
  <%= render :partial => 'inheritance_tree_branch', :locals => {:branches => @symbol.inheritance_tree} %>
</section>

<section id="public_members">
  <h2>Public Members</h2>
  <div id="public_properties">
    <h3>Public Properties</h3>
    <%= render :partial => 'members_table', :locals => {:members => @symbol.public_properties, :member_type => 'property', :member_label => 'Public Property'} %>
  </div>

  <div id="inherited_public_properties">
    <h3>Inherited Public Properties</h3>
    <%= render :partial => 'members_table', :locals => {:members => @symbol.borrowed_public_properties, :member_type => 'property', :member_label => 'Inherited Public Property'} %>
  </div>

  <div id="public_methods">
    <h3>Public Methods</h3>
    <%= render :partial => 'members_table', :locals => {:members => @symbol.public_methods, :member_type => 'method', :member_label => 'Public Method'} %>
  </div>

  <div id="inherited_public_methods">
    <h3>Inherited Public Methods</h3>
    <%= render :partial => 'members_table', :locals => {:members => @symbol.borrowed_public_methods, :member_type => 'method', :member_label => 'Inherited Public Method'} %>
  </div>

  <div id="public_events">
    <h3>Public Events</h3>
    <%= render :partial => 'members_table', :locals => {:members => @symbol.public_events, :member_type => 'event', :member_label => 'Public Event'} %>
  </div>

  <div id="inherited_public_events">
    <h3>Inherited Public Events</h3>
    <%= render :partial => 'members_table', :locals => {:members => @symbol.borrowed_public_events, :member_type => 'event', :member_label => 'Inherited Public Event'} %>
  </div>
</section>

