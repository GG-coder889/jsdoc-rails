<ul>
  <li>
    <% if @symbol == branches.last %>
      <%= branches.pop.name %>
    <% else %>
      <%= link_to_symbol(branches.pop.alias, branches.last.name) %>
    <% end %>
    <% if branches.size > 0 %>
      <%= render :partial => 'inheritance_tree_branch', :locals => {:branches => branches} %>
    <% end %>
  </li>
</ul>
